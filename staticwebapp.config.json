{
    "routes": [
        {
            "route": "/robots.txt",
            "serve": "/robots.txt"
        },
        {
            "route": "/sitemap.xml",
            "serve": "/sitemap.xml"
        },
        {
            "route": "/*",
            "serve": "/index.html"
        }
    ],
    "navigationFallback": {
        "rewrite": "index.html",
        "exclude": ["/assets/*", "*.js", "/robots.txt"]
    }
}